{
  "name": "RiskManagement2",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.1.1",
    "@sap-cloud-sdk/util": "^3.1.1",
    "@sap/cds": "^6",
    "@sap/low-code-event-handler": "^1.2.1",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^3.2.17",
    "express": "^4",
    "hdb": "^0.19.5",
    "passport": "^0.6.0",
    "@sap/mdk-tools": "^1.3.1"
  },
  "devDependencies": {
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana-cloud"
      },
      "db": "sql",
      "app-service": {
        "impl": "@sap/low-code-event-handler"
      },
      "auth": {
        "kind": "xsuaa"
      },
      "ZGWSAMPLE_BASIC": {
        "kind": "odata-v2",
        "model": "srv/external/ZGWSAMPLE_BASIC",
        "[production]": {
          "credentials": {
            "destination": "ES5",
            "path": "/sap/opu/odata/iwbep/GWSAMPLE_BASIC"
          }
        }
      }
    },
    "features": {
      "fetch_csrf": true
    }
  },
  "lcap": true
}